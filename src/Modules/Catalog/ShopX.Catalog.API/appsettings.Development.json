{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "ShopX.Catalog.API"
    }
  },
    "ConnectionStrings": {
        //"Postgres": "Host=localhost;Port=5432;Database=shopxdb;Username=postgres;Password=Admin123",
        "Postgres": "Host=postgres;Port=5432;Database=shopx_catalog;Username=shopx;Password=shopx"
    }
}